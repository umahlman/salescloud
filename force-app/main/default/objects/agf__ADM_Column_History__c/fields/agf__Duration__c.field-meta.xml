<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>agf__Duration__c</fullName>
    <deprecated>false</deprecated>
    <description>Duration with 4 precision points. Here integer part is number of days and fractional part can be converted into hours by multiplying by 24.</description>
    <externalId>false</externalId>
    <formula>IF( ISBLANK(agf__Time_Left__c) , NOW() , agf__Time_Left__c)  -  agf__Time_Entered__c</formula>
    <label>Duration</label>
    <precision>18</precision>
    <required>false</required>
    <scale>4</scale>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
